{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "mounts": [
    "source=jaimemachadoneto-dev-container,target=/nix,type=volume",
    "source=jaimemachadoneto-dev-container-ext,target=/home/vscode/.vscode-server/extensions,type=volume",
    "source=jaimemachadoneto-dev-container-extin,target=/home/vscode/.vscode-server-insiders/extensions,type=volume"
  ],
  "postStartCommand": "cd /workspaces/dev-container && nix develop -c /bin/bash -c \"echo 'Pre-loading complete!'\"",
  "remoteUser": "vscode",
  "settings": {
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.profiles.linux": {
      "zsh": {
        "path": "/home/vscode/.nix-profile/bin/zsh"
      }
    }
  }
}